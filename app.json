{
  "expo": {
    "name": "watch_salon",
    "slug": "watch_salon",
    "scheme": "myapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/shreve_circle.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "androidStatusBar": {
      "hidden": true
    },
    "androidNavigationBar": {
      "visible": "immersive"
    },
    "ios": {
      "bundleIdentifier": "com.connorclose.watchsalon",
      "buildNumber": "1",
      "supportsTablet": true
    },
    "android": {
      "package": "com.connorclose.watchsalon",
      "versionCode": 1,
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "72dd4ec1-c280-4c04-9fea-cfe361271908"
      }
    }
  }
}